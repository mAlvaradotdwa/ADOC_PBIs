	measure 'Fechas Seleccionadas' = ```
			        VAR IniD =
			            FIRSTNONBLANK ( Fecha[Fecha], TRUE () )
			        VAR EndD =
			            LASTNONBLANK ( Fecha[Fecha], TRUE () )
			        VAR totalDays =
			            CALCULATE (
			                DISTINCTCOUNT ( Fecha[Fecha] ),
			                ALL ( Fecha ),
			                Fecha[Fecha] >= IniD,
			                Fecha[Fecha] <= EndD
			            )
			        VAR selectedMonths =
			            CALCULATE ( DISTINCTCOUNT ( Fecha[NumAnoMes] ) )
			        VAR selectedDays =
			            CALCULATE ( DISTINCTCOUNT ( Fecha[Fecha] ) )
			        VAR isContinuum = totalDays = selectedDays -- 
			        VAR IniDateText =
			            FORMAT ( IniD, "dd " ) & UPPER ( LEFT ( FORMAT ( IniD, "MMM" ), 1 ) )
			                & RIGHT ( FORMAT ( IniD, "MMM." ), 3 )
			                & IF ( YEAR ( IniD ) = YEAR ( EndD ), "", FORMAT ( IniD, " yyyy" ) )
			        VAR EndDateText =
			            FORMAT ( EndD, "dd " ) & UPPER ( LEFT ( FORMAT ( EndD, "MMM" ), 1 ) )
			                & RIGHT ( FORMAT ( EndD, "MMM." ), 3 )
			                & FORMAT ( EndD, " yyyy" ) /**/
			        RETURN
			            IF (
			                isContinuum,
			                IF (
			                    selectedMonths = 1,
			                    FORMAT ( IniD, "dd" ) & " al " & EndDateText,
			                    IniDateText & " al " & EndDateText
			                ),
			                "Varias fechas seleccionadas"
			            )
