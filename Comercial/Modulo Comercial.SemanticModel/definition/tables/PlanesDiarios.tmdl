	partition PlanesDiarios-5f9a49b6-6cd3-4828-9fa5-06e6104407ac = calculated
		mode: import
		source =
				
				VAR _minDate =
				    MIN ( PlanesConsolidados[Fecha] )
				VAR _maxDate =
				    MAX ( PlanesConsolidados[Fecha] ) + 365
				VAR _dts =
				    ADDCOLUMNS (
				        CALENDAR ( _minDate, _maxDate ),
				        "@Ano", YEAR ( [Date] ),
				        "@Mes", MONTH ( [Date] )
				    )
				RETURN
				    SUMMARIZE (
				        FILTER (
				            CROSSJOIN ( PlanesConsolidados, _dts ),
				            PlanesConsolidados[Año] = [@Ano]
				                && PlanesConsolidados[Mes] = [@Mes]
				        ),
				        [KPI],
				        [Date],
				        [BV_PAIS_TIENDA],
				        [Plan]
				    )

