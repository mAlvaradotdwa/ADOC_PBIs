	partition Tiempo-f15b074d-32a6-4a23-95f8-242fc5d7644a = calculated
		mode: import
		source =
				
				ADDCOLUMNS (
				    CROSSJOIN (
				        FILTER (
				            UNION ( DATATABLE ( "HoraNum", INTEGER, { { } } ), GENERATESERIES ( 0, 23 ) ),
				            ISBLANK ( [HoraNum] ) = FALSE ()
				        ),
				        FILTER (
				            UNION (
				                DATATABLE ( "BloqueMinutos", INTEGER, { { } } ),
				                GENERATESERIES ( 0, 3 )
				            ),
				            ISBLANK ( [BloqueMinutos] ) = FALSE ()
				        )
				    ),
				    "Hora", FORMAT ( [HoraNum], "00" ),
				    "Minutos", FORMAT ( [BloqueMinutos] * 15, "00" ),
				    "BloqueHora",
				        FORMAT ( [HoraNum], "00" ) & ":"
				            & FORMAT ( [BloqueMinutos] * 15, "00" ),
				    "IDTiempo",
				        [HoraNum] * 100 + ( [BloqueMinutos] * 15 ),
				    "Hora Sucursales",
				        VAR _currentDay = /*DATE(2022,12,15) --*/ TODAY ()
				        VAR _openingDefault =
				            LOOKUPVALUE (
				                _ref_HorariosExtendidos[HoraApertura],
				                _ref_HorariosExtendidos[Nombre], "Default"
				            )
				        VAR _ClosingDefault =
				            LOOKUPVALUE (
				                _ref_HorariosExtendidos[HoraCierre],
				                _ref_HorariosExtendidos[Nombre], "Default"
				            )
				        VAR _openingTime =
				            LOOKUPVALUE (
				                _ref_HorariosExtendidos[HoraApertura],
				                _ref_HorariosExtendidos[Fecha], _currentDay,
				                _openingDefault
				            )
				        VAR _closingTime =
				            LOOKUPVALUE (
				                _ref_HorariosExtendidos[HoraCierre],
				                _ref_HorariosExtendidos[Fecha], _currentDay,
				                _closingDefault
				            )
				        RETURN
				            IF (
				                [HoraNum] >= _openingTime
				                    && [HoraNum] <= _closingTime,
				                FORMAT ( [HoraNum], "00" ),
				                "Otra"
				            )
				)

	annotation PBI_Id = fcf0a5a8caac499091cb97b186e7f19f

