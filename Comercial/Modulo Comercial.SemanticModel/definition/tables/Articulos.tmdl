                               FROM [dbo].[FAC_VENTARPRO] [f]#(lf)                                  --JOIN [DM_COMERCIAL].[dbo].[DIM_ARTICULOSAP] AS [A1]#(lf)                                  --     ON [f].[SK_MATNR] = [A1].[SK_MATNR]#(lf)                                  WHERE [f].[SK_PERIODO] >= @InitialDate#(lf)                                 --AND [f].[SK_PERIODO] < @Today#(lf)                                 --C=CAL (calzado), R=ROP (ropa), A=ACC (accesorios)#(lf)                                 --AND [A1].[SK_DEPTO] IN ( 'C' , 'R' , 'A') #(lf)                                 UNION#(lf)                                 SELECT DISTINCT #(lf)                                        [SK_MATNR_REF] = MIN([fac].[SK_MATNR])#(lf)                                   FROM [dbo].[FAC_INVENTARIOM] [fac]#(lf)                                   INNER JOIN [DIM_ARTICULOSAP] [A]#(lf)                                        ON [fac].[SK_MATNR] = [a].[SK_MATNR]#(lf)                                  WHERE [SK_PERIODO] >= @InitialDate#(lf)                                        AND [SK_PERIODO] < @Today#(lf)                                        AND [fac].[SK_ALMACEN] NOT IN#(lf)                                                                      ( 'BP05' , #(lf)                                                                        'BP08' , #(lf)                                                                        'SU01'#(lf)                                                                      )#(lf)                                        AND [A].[SK_DEPTO] IN ( 'C' , 'R' , 'A' )#(lf)                                  GROUP BY #(lf)                                           [fac].[SK_PERIODO] , #(lf)                                           [fac].[SK_TIENDA] , #(lf)                                           [A].[BV_MARCA] , #(lf)                                           [A].[BV_DEPTO] , #(lf)                                           [A].[BV_GENERO] , #(lf)                      