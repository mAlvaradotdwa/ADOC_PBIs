	partition Proveedores = calculated
		mode: import
		source =
				
				VAR _artProv =
				    SUMMARIZE (
				        ArticuloProveedor,
				        ArticuloProveedor[SK_UNIDADNEGOCIO],
				        ArticuloProveedor[BV_DEPTO],
				        ArticuloProveedor[SK_MARCA],
				        ArticuloProveedor[BV_GENERO],
				        ArticuloProveedor[BV_CATEGORIA],
				        ArticuloProveedor[NombreProveedor],
				        "art_Key",
				            ArticuloProveedor[SK_UNIDADNEGOCIO] & ArticuloProveedor[BV_DEPTO] & ArticuloProveedor[SK_MARCA] & ArticuloProveedor[BV_GENERO] & ArticuloProveedor[BV_CATEGORIA],
				        "nr", COUNTROWS ( ArticuloProveedor )
				    )
				VAR _art =
				    SUMMARIZE (
				        ArticuloProveedor,
				        ArticuloProveedor[SK_UNIDADNEGOCIO],
				        ArticuloProveedor[BV_DEPTO],
				        ArticuloProveedor[SK_MARCA],
				        ArticuloProveedor[BV_GENERO],
				        ArticuloProveedor[BV_CATEGORIA],
				        "art_Key",
				            ArticuloProveedor[SK_UNIDADNEGOCIO] & ArticuloProveedor[BV_DEPTO] & ArticuloProveedor[SK_MARCA] & ArticuloProveedor[BV_GENERO] & ArticuloProveedor[BV_CATEGORIA],
				        "Articulos", COUNTROWS ( ArticuloProveedor )
				    )
				RETURN
				    ADDCOLUMNS (
				        _art,
				        "NombreProveedor",
				            VAR _sv = [art_key]
				            RETURN
				                MAXX (
				                    TOPN ( 1, FILTER ( _artProv, [art_key] = _sv ), [nr], DESC ),
				                    [NombreProveedor]
				                )
				    )

