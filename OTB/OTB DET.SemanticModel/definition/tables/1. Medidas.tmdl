	measure 'Inventario USD BOM' = ```
			
			VAR BOM =
			CALCULATE(SUM('OTB'[Inventario USD Mes]) + SUM('OTB'[Inventario USD Arribo]),
			    'Calendario'[Año] = YEAR(MAX('OTB Max Fecha'[Max Fecha])),
			    'Calendario'[Mes] = MONTH(MAX('OTB Max Fecha'[Max Fecha])),
			    'OTB'[Fuente] = "OTB")
			VAR VTA = 
			CALCULATE([Venta USD Plan],
			    'Calendario'[Fecha] < MAX('Calendario'[Fecha]),
			    'Calendario'[Fecha] >= MAX('OTB Max Fecha'[Max Fecha]) + 1) - [Venta USD Plan]
			VAR MKD = 
			CALCULATE([Markdown USD Plan],
			    'Calendario'[Fecha] < MAX('Calendario'[Fecha]),
			    'Calendario'[Fecha] >= MAX('OTB Max Fecha'[Max Fecha]) + 1) - [Markdown USD Plan]
			VAR ARR =
			CALCULATE([Arribos Totales],
			    'Calendario'[Fecha] < MAX('Calendario'[Fecha]),
			    'Calendario'[Fecha] >= MAX('OTB Max Fecha'[Max Fecha]) + 1) - [Arribos Totales]
			RETURN
			IF(ISBLANK(SELECTEDVALUE('Calendario'[Mes])), BLANK(),
			    IF(ISBLANK([Venta USD]),
			        BOM - VTA - MKD + ARR,
			        CALCULATE([Inventario USD EOM], DATEADD('Calendario'[Fecha], -1, MONTH))))
			```
