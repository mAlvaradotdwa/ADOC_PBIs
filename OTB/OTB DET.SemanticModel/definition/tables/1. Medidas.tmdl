	measure 'Cobertura Target' = ```
			
			VAR Inventario = [Inventario USD Target]
			VAR Tabla1 = 
			ADDCOLUMNS(
			    GROUPBY(FILTER(ALL('Calendario'), ([Año] = MAX('Calendario'[Año]) && [Mes] > MAX('Calendario'[Mes])) || ([Año] > MAX('Calendario'[Año]))), [Año], [Mes]),
			    "Fecha", EOMONTH(DATE([Año], [Mes], 1), 0))
			VAR Tabla3 = ADDCOLUMNS(Tabla1, "Diferencia", Inventario - SUMX(FILTER(Tabla1, [Fecha] <= EARLIER([Fecha])), [Venta USD + Plan]))
			VAR Dias = DIVIDE(
			    MINX(FILTER(Tabla3, [Diferencia] >= 0), [Diferencia]),
			    MINX(FILTER(Tabla3, [Diferencia] >= 0), [Diferencia]) + ABS(MAXX(FILTER(Tabla3, [Diferencia] < 0), [Diferencia])))
			VAR Demanda = MAXX(FILTER(Tabla3, [Diferencia] = MAXX(FILTER(Tabla3, [Diferencia] < 0), [Diferencia])), [Venta USD + Plan])
			VAR MC = IF(ISBLANK(COUNTROWS(FILTER(Tabla3, [Diferencia] >= 0)) + Dias), DIVIDE(Inventario, Demanda), COUNTROWS(FILTER(Tabla3, [Diferencia] >= 0)) + Dias)
			RETURN IF(NOT(ISBLANK(Inventario)), MC)
			```
