	partition DIM_ARTICULOS = m
		mode: import
		source =
				let
				    Origen = Sql.Database("192.168.16.74", "DM_COMERCIAL", [Query="WITH JerMarca AS (#(lf)    SELECT#(lf)        A.""BV_MARCA"",#(lf)        CASE#(lf)            WHEN A.""BV_MARCA"" IN ('4X4','501','BRA','KIP','KIC','MIR','VEL','PR2','BRI','NIP','USA','DIN')#(lf)                THEN 'PAR2'#(lf)            WHEN A.""BV_MARCA"" IN ('ADO','HEA','COY','DIS','DSP','STM','LCS')#(lf)                THEN 'ADOC'#(lf)            WHEN A.""BV_MARCA"" IN ('CAT','HPU','TNF','MER','VNS')#(lf)                THEN A.""BV_MARCA""#(lf)            ELSE 'OTH'#(lf)        END AS ""MARCA_GR""#(lf)    FROM ""DM_COMERCIAL"".""dbo"".""DIM_ARTICULOSAP"" A#(lf)    GROUP BY A.""BV_MARCA""#(lf))#(lf)SELECT #(lf)    A.*,#(lf)    J.""MARCA_GR""#(lf)FROM ""DM_COMERCIAL"".""dbo"".""DIM_ARTICULOSAP"" A#(lf)LEFT JOIN JerMarca J #(lf)       ON A.""BV_MARCA"" = J.""BV_MARCA""#(lf)WHERE A.""TIPOESTILO"" IS NOT NULL#(lf)  AND A.""MATNR_BORRADO"" = ''"])
				in
				    Origen

	annotation PBI_NavigationStepName = Navegación

