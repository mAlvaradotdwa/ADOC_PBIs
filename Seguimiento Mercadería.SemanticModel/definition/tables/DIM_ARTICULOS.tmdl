	column GENERICO = ```
			VAR _position =
			    SEARCH ( "0000000", DIM_ARTICULOS[SK_MATNR], 1, -1 )
			RETURN
			    IF ( 
			        _position > -1, 
			        MID ( DIM_ARTICULOS[SK_MATNR], _position + 7, 8 ) 
			    )
