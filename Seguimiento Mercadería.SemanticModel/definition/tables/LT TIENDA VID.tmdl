	partition 'LT TIENDA VID' = m
		mode: import
		source =
				let
				    Consulta1 = Value.NativeQuery(SapHana.Database("vhadoahpdb01.hec.empresasadoc.com:30215", [Implementation="2.0"]), "-- NUEVA DATA LT TIENDA --#(lf)WITH BASE AS (#(lf)    SELECT #(lf)        CASE WHEN M.BUKRS IN ('PA','SV') THEN NULL ELSE L.VBELN END AS Referencia,#(lf)        CASE WHEN M.BUKRS IN ('PA','SV') THEN NULL ELSE L.TRAID END AS VID,#(lf)        CASE WHEN M.BUKRS IN ('PA','SV') THEN NULL ELSE L.WADAT END AS FechaCreacionVID,#(lf)        CASE WHEN M.BUKRS IN ('PA','SV') THEN NULL ELSE L.AEDAT END AS FechaCierreVID,#(lf)#(lf)        M.MATNR         AS Articulo,#(lf)        MAKT.MAKTX      AS DescripcionArticulo,#(lf)        M.WERKS         AS Tienda,#(lf)        T.NAME1         AS NombreTienda,#(lf)        M.EBELN         AS Documento,#(lf)        M.MENGE         AS Cantidad,#(lf)        M.MEINS         AS UnidadMedida,#(lf)        M.BUKRS         AS Pais,#(lf)        M.BUDAT_MKPF    AS FechaContabilizacion,#(lf)        M.CPUDT_MKPF    AS FechaIngreso,#(lf)        M.VBELN_IM      AS ReferenciaMSEG,#(lf)        M.VBELP_IM      AS PosicionReferencia,#(lf)#(lf)        P.CREATIONDATE  AS FechaCreacionDocumentoEKPO,#(lf)        P.CREATIONTIME  AS HoraCreacionDocumentoEKPO,   #(lf)        P.RESLO         AS AlmacenReceptor,#(lf)#(lf)        K.AEDAT         AS FechaCreacionDocumentoEKKO,#(lf)        K.RESWK         AS CentroSuministrador,#(lf)#(lf)        CASE #(lf)            WHEN P.CREATIONTIME > '120000' #(lf)                THEN ADD_DAYS(K.AEDAT, 1)#(lf)            ELSE K.AEDAT#(lf)        END AS FechaAjustada,#(lf)#(lf)        CASE #(lf)            WHEN M.BUKRS IN ('PA','SV') THEN NULL#(lf)            ELSE DAYS_BETWEEN(L.WADAT, L.AEDAT)#(lf)        END AS LeadTime_Creacion_Cierre,#(lf)#(lf)        CASE #(lf)            WHEN M.BUKRS IN ('PA','SV') THEN NULL#(lf)            ELSE DAYS_BETWEEN(L.WADAT, M.BUDAT_MKPF)#(lf)        END AS LeadTime_Creacion_Contabilizacion,#(lf)#(lf)        DAYS_BETWEEN(#(lf)            CASE #(lf)                WHEN P.CREATIONTIME > '120000' #(lf)                    THEN ADD_DAYS(K.AEDAT, 1)#(lf)                ELSE K.AEDAT#(lf)            END,#(lf)            M.BUDAT_MKPF#(lf)        ) AS LeadTime_Doc_Contabilizacion,#(lf)#(lf)        CASE #(lf)            WHEN M.BUKRS IN ('PA','SV') THEN NULL#(lf)            ELSE SUM(DAYS_BETWEEN(L.WADAT, L.AEDAT) * M.MENGE) OVER (PARTITION BY L.TRAID) #(lf)                 / NULLIF(SUM(M.MENGE) OVER (PARTITION BY L.TRAID),0)#(lf)        END AS LeadTime1_Ponderado,#(lf)#(lf)        CASE #(lf)            WHEN M.BUKRS IN ('PA','SV') THEN NULL#(lf)            ELSE SUM(DAYS_BETWEEN(L.WADAT, M.BUDAT_MKPF) * M.MENGE) OVER (PARTITION BY L.TRAID) #(lf)                 / NULLIF(SUM(M.MENGE) OVER (PARTITION BY L.TRAID),0)#(lf)        END AS LeadTime2_Ponderado,#(lf)#(lf)        SUM(DAYS_BETWEEN(#(lf)                CASE #(lf)                    WHEN P.CREATIONTIME > '120000' #(lf)                        THEN ADD_DAYS(K.AEDAT, 1)#(lf)                    ELSE K.AEDAT#(lf)                END,#(lf)                M.BUDAT_MKPF#(lf)            ) * M.MENGE#(lf)        ) OVER (PARTITION BY L.TRAID) #(lf)        / NULLIF(SUM(M.MENGE) OVER (PARTITION BY L.TRAID),0) AS LeadTime3_Ponderado#(lf)#(lf)    FROM SAP_ECC.LIKP L#(lf)    INNER JOIN SAP_ECC.MSEG M #(lf)        ON M.VBELN_IM = L.VBELN#(lf)        AND M.BWART = '101'#(lf)        AND M.LGORT = 'AL01'#(lf)#(lf)    INNER JOIN SAP_ECC.EKPO P#(lf)        ON P.EBELN = M.EBELN#(lf)        AND P.EBELP = M.EBELP#(lf)        AND P.LOEKZ <> 'L'#(lf)        AND P.MTART IN ('MODE','ZMOD')#(lf)#(lf)    INNER JOIN SAP_ECC.EKKO K#(lf)        ON K.EBELN = M.EBELN#(lf)        AND K.BSART IN ('UB', 'ZTR2', 'ZREP')#(lf)#(lf)    LEFT JOIN SAP_ECC.T001W T#(lf)        ON M.WERKS = T.WERKS#(lf)#(lf)    LEFT JOIN SAP_ECC.MAKT MAKT#(lf)        ON M.MATNR = MAKT.MATNR#(lf)        AND MAKT.SPRAS = 'S'#(lf)#(lf)    WHERE L.TRAID IS NOT NULL#(lf)      AND L.AEDAT >= '20240101'#(lf)      AND L.AEDAT <= CURRENT_DATE#(lf)),#(lf)#(lf)#(lf)PEDIDOS_AGRUPADOS AS (#(lf)    SELECT#(lf)        CentroSuministrador,#(lf)        Tienda,#(lf)        Documento AS Pedido,#(lf)        COUNT(DISTINCT VID) AS Cantidad_Despachos,#(lf)        SUM(Cantidad) AS UnidadesTotales,#(lf)        CASE WHEN COUNT(DISTINCT VID) > 1 THEN 1 ELSE 0 END AS Pedido_Partido_Flag#(lf)    FROM BASE#(lf)    GROUP BY CentroSuministrador, Tienda, Documento#(lf))#(lf)#(lf)SELECT#(lf)    B.*,#(lf)    A.Cantidad_Despachos,#(lf)    A.UnidadesTotales,#(lf)    A.Pedido_Partido_Flag#(lf)FROM BASE B#(lf)LEFT JOIN PEDIDOS_AGRUPADOS A#(lf)    ON B.CentroSuministrador = A.CentroSuministrador#(lf)    AND B.Tienda = A.Tienda#(lf)    AND B.Documento = A.Pedido#(lf)ORDER BY B.CentroSuministrador, B.Tienda, B.Documento, B.Articulo#(lf)", null, [EnableFolding=true]),
