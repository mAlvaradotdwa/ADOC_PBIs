		summarizeBy: none

		annotation SummarizationSetBy = Automatic

	column 'Pais Destino' =
			VAR _codPais =
			    LEFT ( MK_LOTES[ZCENTRODES], 1 )
			RETURN
			    CALCULATE (
			        MIN ( DIM_PAIS[BV_PAIS] ),
			        LEFT ( DIM_PAIS[SK_PAIS], 1 ) = _codPais
			    )
