	partition Consulta1 = m
		mode: import
		queryGroup: 'Fuentes SAP'
		source = Value.NativeQuery(SapHana.Database("vhadoahpdb01.hec.empresasadoc.com:30215", [Implementation="2.0"]), "SELECT #(lf)       EKKO.EBELN                 Doc_compra , #(lf)       EKPO.EBELP                 posicion , #(lf)       EKPO.MATNR                 articulo , #(lf)       EKET.MENGE                 cantidad_reparto , #(lf)       -- EKPO.MEINS                 unidad , #(lf)       UnDsc.MSEH3                unidad , #(lf)       EKET.GLMNG                 entregas , #(lf)       EKET.WAMNG                 cantidad_salida , #(lf)       EKET.WEMNG                 cantidad_ingresada , #(lf)       EKKO.BSART                 clase_doc , #(lf)       EKKO.AEDAT                 creado_el , #(lf)       EKKO.ERNAM                 creado_por , #(lf)       EKKO.RESWK                 centro_suminis , #(lf)       EKPO.TXZ01                 Texto_breve , #(lf)       EKPO.WERKS                 centro , #(lf)       MARA.MTART                 tipo_material , #(lf)       MARA.GROES                 estilo , #(lf)       MARA.SATNR                 mat_gral_conf , #(lf)       MARA.FREE_CHAR             color , #(lf)       MARA.SIZE1                 talla , #(lf)       MARA.BRAND_ID              marca , #(lf)       MARA.ZZ_ART_PROV           genero , #(lf)       T001W.NAME1                tienda , #(lf)       T001W.LAND1                pais , #(lf)       EKPO.LOEKZ                 indicador_de_borrado , #(lf)       MARA.ZZ_DEPART             departamento , #(lf)       EKKO.ZZ_TIPO_FLETE         tipo_flete , #(lf)       EKKO.VERKF                 vendedor , #(lf)       EKPO.RESLO                 almacen_emisor , #(lf)       EKPO.LGORT                 almacen , #(lf)       ZMM_EXPED_CONTS.CONTENEDOR contenedor , #(lf)       EKKO.ZZF_DESPACHO          fecha , #(lf)       EKPO.LBELN                 NO_expediente_ekpo , #(lf)       ZMM_EXPED_IMP.LBELN        no_expediente , #(lf)       EKPO.FECHA_EXP             fecha_expediente , #(lf)       EKPO.FECHA_LIQ             fecha_liquidacion , #(lf)       EKET.CHARG                 lote , #(lf)       EKKO.BEDAT                 fecha_documento , #(lf)       EKKO.EKGRP                 grupo_de_compra , #(lf)       MARA.EAN11                 upc , #(lf)       EKKO.FRGKE                 indicador_de_liquidacion , #(lf)       EKPO.ELIKZ                 entrega_final ,#(lf)       CASE#(lf)          WHEN ekpo.attyp = '11' THEN CAST(#(lf)               REPLACE(SUBSTRING(ekpo.txz01, 9, 2), '-', '') AS int#(lf)          )#(lf)       ELSE 1#(lf)     END * EKET.MENGE             CantidadReparto_total#(lf)  FROM SAP_ECC.EKKO#(lf)  INNER JOIN SAP_ECC.EKPO#(lf)       ON EKPO.EBELN = EKKO.EBELN#(lf)  LEFT OUTER JOIN SAP_ECC.ZMM_EXPED_IMP#(lf)       ON ZMM_EXPED_IMP.LBELN = EKPO.LBELN#(lf)  LEFT OUTER JOIN SAP_ECC.ZMM_EXPED_CONTS#(lf)       ON ZMM_EXPED_CONTS.LBELN = EKPO.LBELN#(lf)  INNER JOIN SAP_ECC.EKET#(lf)       ON EKET.EBELN = EKPO.EBELN#(lf)          AND EKET.EBELP = EKPO.EBELP#(lf)  INNER JOIN SAP_ECC.MARA#(lf)       ON MARA.MATNR = EKPO.MATNR#(lf)  INNER JOIN SAP_ECC.T001W#(lf)       ON T001W.WERKS = EKPO.WERKS#(lf)LEFT JOIN SAP_ECC.T006A UnDsc#(lf)#(tab)ON EKPO.MEINS = UnDsc.MSEHI #(lf) WHERE #(lf)#(tab)EKKO.AEDAT BETWEEN TO_VARCHAR(ADD_DAYS(LAST_DAY(ADD_MONTHS(CURRENT_DATE , -4)) , 1) , 'YYYYMMDD') AND TO_VARCHAR(LAST_DAY(CURRENT_DATE) , 'YYYYMMDD')#(lf)#(tab)AND EKPO.LOEKZ <> 'L'#(lf)#(tab)AND (#(lf)#(tab)#(tab)(#(lf)#(tab)#(tab) #(tab)EKKO.BSART IN ( 'ZINT' )#(lf)#(tab)       #(tab)AND EKKO.RESWK IN ( '8010' , '1010' , '2010' )#(lf)#(tab)#(tab)#(tab)AND MARA.MTART IN ( 'MODE' )#(lf)#(tab)   #(tab)) OR (#(lf)#(tab)   #(tab)#(tab)EKKO.BSART IN ( 'ZIMP' )#(lf)#(tab)   #(tab)) #(lf)#(tab))#(lf)#(tab)-- Filtro Aplicado por solicitud de Jose Mena#(lf)#(tab)AND UnDsc.SPRAS = 'S'#(lf)#(tab)AND UnDsc.MSEH3 IN ('PAR','UN')#(lf)#(tab)-- -- Para validación de conversion de columnas#(lf)#(tab)-- AND EKKO.EBELN = '4700157612'#(lf)#(lf)#(tab)#(lf)#(tab)", null, [EnableFolding=true])

