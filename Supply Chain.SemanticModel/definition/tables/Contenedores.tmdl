	column UN_NEGOCIO =
			VAR _codMarca = Contenedores[MARCA]
			RETURN
			    CALCULATE (
			        MIN ( Articulos[SK_UNIDADNEGOCIO] ),
			        Articulos[BV_MARCA] = _codMarca
			    )
