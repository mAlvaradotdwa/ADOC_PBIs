			VAR _lastDate =
			    CALCULATE (
			            MAXX (
			                {
			                    MAX ( Inventarios[PERIODO] ),
			                    MAX ( 'zsap_PREPARACIONES'[FECHA] )
			                },
			                [Value]
			            ),
			            ALL ()
			        )
			RETURN [Fecha] <= _lastDate
